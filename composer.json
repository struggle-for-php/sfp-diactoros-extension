{
  "name" : "struggle-for-php/sfp-diactoros-extension",
  "description" : "An extension for zend-diactoros.",
  "require" : {
    "php" : "^5.6 || ^7.0",
    "zendframework/zend-diactoros" : "~1.3"
  },
  "require-dev" : {
    "phpunit/phpunit" : "^5",
    "lstrojny/phpunit-function-mocker" : "~0.4",
    "squizlabs/php_codesniffer": "~2.0"
  },
  "authors" : [ {
    "name" : "sasezaki",
    "email" : "sasezaki@gmail.com",
    "homepage" : "http://github.com/sasezaki",
    "role" : "Developer"
  } ],
  "autoload-dev" : {
    "psr-4" : {
      "SfpDiactorosTest\\" : "tests/SfpDiactorosTest"
    }
  },
  "keywords" : [ "diactoros", "zend-diactoros" ],
  "autoload" : {
    "psr-4" : {
      "SfpDiactoros\\" : "src/SfpDiactoros"
    }
  },
  "license" : "BSD-3-Clause"
}